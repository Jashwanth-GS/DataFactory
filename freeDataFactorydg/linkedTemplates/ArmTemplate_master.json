{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "freeDataFactorydg"
		},
		"SqlServer1_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'SqlServer1'"
		},
		"AzureBlobStorage2_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'AzureBlobStorage2'"
		},
		"AmazonS31_properties_typeProperties_accessKeyId": {
			"type": "object",
			"defaultValue": {
				"type": "AzureKeyVaultSecret",
				"store": {
					"referenceName": "AzureKeyVault1",
					"type": "LinkedServiceReference"
				},
				"secretName": "s3accessid"
			}
		},
		"AzureBatch1_properties_typeProperties_accountName": {
			"type": "string",
			"defaultValue": "freebatchaccount"
		},
		"AzureBatch1_properties_typeProperties_batchUri": {
			"type": "string",
			"defaultValue": "https://freebatchaccount.eastus.batch.azure.com"
		},
		"AzureBatch1_properties_typeProperties_poolName": {
			"type": "string",
			"defaultValue": "freepool"
		},
		"AzureBatch2_properties_typeProperties_accountName": {
			"type": "string",
			"defaultValue": "freebatchaccount"
		},
		"AzureBatch2_properties_typeProperties_batchUri": {
			"type": "string",
			"defaultValue": "https://freebatchaccount.eastus.batch.azure.com"
		},
		"AzureBatch2_properties_typeProperties_poolName": {
			"type": "string",
			"defaultValue": "freepool"
		},
		"AzureBlobStorage1_properties_typeProperties_connectionString_secretName": {
			"type": "string",
			"defaultValue": "storage-account-connection-string"
		},
		"AzureKeyVault1_properties_typeProperties_baseUrl": {
			"type": "string",
			"defaultValue": "https://myfreekeyvault.vault.azure.net/"
		},
		"AzureBatch3_properties_typeProperties_accountName": {
			"type": "string",
			"defaultValue": "freebatchaccount"
		},
		"AzureBatch3_properties_typeProperties_batchUri": {
			"type": "string",
			"defaultValue": "https://freebatchaccount.eastus.batch.azure.com"
		},
		"AzureBatch3_properties_typeProperties_poolName": {
			"type": "string",
			"defaultValue": "new_pool_activity"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "freeDataFactorydg_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"AzureBlobStorage2_connectionString": {
						"value": "[parameters('AzureBlobStorage2_connectionString')]"
					},
					"SqlServer1_connectionString": {
						"value": "[parameters('SqlServer1_connectionString')]"
					},
					"AzureKeyVault1_properties_typeProperties_baseUrl": {
						"value": "[parameters('AzureKeyVault1_properties_typeProperties_baseUrl')]"
					},
					"AmazonS31_properties_typeProperties_accessKeyId": {
						"value": "[parameters('AmazonS31_properties_typeProperties_accessKeyId')]"
					},
					"AzureBlobStorage1_properties_typeProperties_connectionString_secretName": {
						"value": "[parameters('AzureBlobStorage1_properties_typeProperties_connectionString_secretName')]"
					},
					"AzureBatch1_properties_typeProperties_accountName": {
						"value": "[parameters('AzureBatch1_properties_typeProperties_accountName')]"
					},
					"AzureBatch1_properties_typeProperties_batchUri": {
						"value": "[parameters('AzureBatch1_properties_typeProperties_batchUri')]"
					},
					"AzureBatch1_properties_typeProperties_poolName": {
						"value": "[parameters('AzureBatch1_properties_typeProperties_poolName')]"
					},
					"AzureBatch2_properties_typeProperties_accountName": {
						"value": "[parameters('AzureBatch2_properties_typeProperties_accountName')]"
					},
					"AzureBatch2_properties_typeProperties_batchUri": {
						"value": "[parameters('AzureBatch2_properties_typeProperties_batchUri')]"
					},
					"AzureBatch2_properties_typeProperties_poolName": {
						"value": "[parameters('AzureBatch2_properties_typeProperties_poolName')]"
					},
					"AzureBatch3_properties_typeProperties_accountName": {
						"value": "[parameters('AzureBatch3_properties_typeProperties_accountName')]"
					},
					"AzureBatch3_properties_typeProperties_batchUri": {
						"value": "[parameters('AzureBatch3_properties_typeProperties_batchUri')]"
					},
					"AzureBatch3_properties_typeProperties_poolName": {
						"value": "[parameters('AzureBatch3_properties_typeProperties_poolName')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}